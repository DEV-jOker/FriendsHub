<table class="table table-striped table-hover" id='user-friends'>
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">Posts</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user|%>
      <tr id="user-<%= user.id %>">
        <th scope="row"><%= user.name %></th>
        <td><%= user.email %></td>
        <td><%#= %></td>
        <td id="td-user-<%= user.id%>">
          <%= render 'users/friend_button',user:user %>
        </td>
      </tr>
    <%end%>
  </tbody>
</table>
